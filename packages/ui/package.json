{
  "name": "@csmm/ui",
  "version": "0.0.0",
  "private": false,
  "author": "CSMM Team",
  "description": "CSMM Lib is a simple and customizable component library to build React apps faster within the csmm eco system",
  "main": "build/src/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "npm run build:tsc && npm run build:storybook",
    "build:tsc": "tsc --project ./tsconfig.production.json && npm run copypackage",
    "copypackage": "cp -rf package.json build",
    "start": "start-storybook -p 6006 --no-manager-cache",
    "build:storybook": "build-storybook --no-manager-cache",
    "lint:package": "sort-package-json"
  },
  "dependencies": {
    "@storybook/addon-viewport": "^6.2.9",
    "framer-motion": "^4.0.3",
    "history": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@types/history": "^4.7.8",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.9",
    "sort-package-json": "^1.49.0"
  }
}
